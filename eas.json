        "EXPO_PUBLIC_VIBECODE_GOOGLE_API_KEY": "${EXPO_PUBLIC_VIBECODE_GOOGLE_API_KEY}",
        "EXPO_PUBLIC_VIBECODE_GROK_API_KEY": "${EXPO_PUBLIC_VIBECODE_GROK_API_KEY}",
        "EXPO_PUBLIC_VIBECODE_ELEVENLABS_API_KEY": "${EXPO_PUBLIC_VIBECODE_ELEVENLABS_API_KEY}"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_VIBECODE_OPENROUTER_API_KEY": "${EXPO_PUBLIC_VIBECODE_OPENROUTER_API_KEY}",
        "EXPO_PUBLIC_VIBECODE_OPENAI_API_KEY": "${EXPO_PUBLIC_VIBECODE_OPENAI_API_KEY}",
        "EXPO_PUBLIC_VIBECODE_ANTHROPIC_API_KEY": "${EXPO_PUBLIC_VIBECODE_ANTHROPIC_API_KEY}",
        "EXPO_PUBLIC_VIBECODE_GOOGLE_API_KEY": "${EXPO_PUBLIC_VIBECODE_GOOGLE_API_KEY}",
        "EXPO_PUBLIC_VIBECODE_GROK_API_KEY": "${EXPO_PUBLIC_VIBECODE_GROK_API_KEY}",
        "EXPO_PUBLIC_VIBECODE_ELEVENLABS_API_KEY": "${EXPO_PUBLIC_VIBECODE_ELEVENLABS_API_KEY}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
